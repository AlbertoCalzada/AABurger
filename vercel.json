{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      },
      {
        "src": "server/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/auth/(.*)",
        "dest": "/api/auth/$1"
      },
      {
        "src": "/api/password/(.*)",
        "dest": "/api/password/$1"
      },
      {
        "src": "/api/send",
        "dest": "/api/send"
      },
      {
        "src": "/api/(.*)",
        "dest": "/server/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  